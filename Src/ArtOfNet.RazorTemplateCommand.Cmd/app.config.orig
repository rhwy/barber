<?xml version="1.0"?>
<configuration>
    <configSections>
        <section name="RazorCommand" type="ArtOfNet.RazorTemplateCommand.Configuration.RazorCommandConfigurationSection, ArtOfNet.RazorTemplateCommand.Configuration"/>
    </configSections>
    <appSettings>
        <add key="DefaultTempFolder" value=".\TempGenerated"/>
    </appSettings>
    <RazorCommand>
        <IncludedAssemblies>
            <add Name="System.dll"/>
            <add Name="System.Core.dll"/>
            <add Name="System.Data.dll"/>
            <add Name="Microsoft.CSharp.dll"/>
        </IncludedAssemblies>
    </RazorCommand>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
</configuration>
